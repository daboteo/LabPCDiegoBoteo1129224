nota = int(input(" Ingrese su nota: "))

if nota < 0 or nota> 100:
    print("Nota no valida" )
else: 
    if nota>=0 and nota<=20:
        print("Facultad 1 ")
    elif nota>=21 and nota<=40:
        print("Facultad 2 ")
    elif nota>=40 and nota<=60:
        print("Facultad 3 ")
    elif nota>=61 and nota<=80:
        print("Facultad 4 ")
    else: 
        print("Facultad 5 ")